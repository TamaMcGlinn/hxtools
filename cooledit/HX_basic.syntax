	keyword \{0123456789\} cyan
	keyword .\{0123456789\} cyan
	keyword wholeright &H+ cyan

	keyword ^ white
	keyword ( brightcyan
	keyword ) brightcyan
	keyword [ brightcyan
	keyword ] brightcyan
	keyword { brightcyan
	keyword } brightcyan
	keyword ; brightcyan
